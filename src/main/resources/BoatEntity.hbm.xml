<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.github.adam6806.catamaranscraper.dao.BoatEntity" table="boat" schema="catamarans">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="identity" />
        </id>
        <property name="location">
            <column name="location" sql-type="varchar(200)" length="200"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(20000)" length="20000" not-null="true"/>
        </property>
        <property name="url">
            <column name="url" sql-type="varchar(500)" length="500"/>
        </property>
        <property name="makeModel">
            <column name="make_model" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="bigint(20)"/>
        </property>
        <property name="length">
            <column name="length" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="year">
            <column name="year" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="dougRating">
            <column name="doug_rating" sql-type="int(11)"/>
        </property>
        <property name="adamRating">
            <column name="adam_rating" sql-type="int(11)"/>
        </property>
        <property name="active">
            <column name="active" sql-type="tinyint(1)"/>
        </property>
        <property name="timestamp">
            <column name="timestamp" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>